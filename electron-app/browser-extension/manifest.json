{
  "manifest_version": 3,
  "name": "Cronus URL Tracker",
  "version": "1.0.0",
  "description": "Securely track active browser tab for Cronus time tracking",
  "permissions": ["tabs", "nativeMessaging"],
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
